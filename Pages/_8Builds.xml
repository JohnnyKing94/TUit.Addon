<GuiXml>
	<Controls>
		<Control name="DynamicTextBuilds"  virtual="true" >
			<Dimensions x="920" y="680" />
			<Controls>
				<Control name="$(parent)Container" inherits="ZO_ScrollContainer">
					<Anchor point="TOPLEFT" offsetY="12" />
					<Anchor point="BOTTOMRIGHT" offsetY="-12" />
                </Control>
			</Controls> 
		</Control>

		<!-- BUILDS LIST -->
		<Control name="DynamicScrollPageBuilds" virtual="true" >
			<Anchor point="TOP" relativePoint="TOP" offsetX="0" relativeTo="$(parent)" />
			<Dimensions x="900" y="200" />
			<Controls>
				<Label name="$(parent)Label" mouseEnabled="true" font="ZoFontWinH1" inheritAlpha="true" color="FFFFFF" wrapMode="TRUNCATE" verticalAlignment="TOP" horizontalAlignment="CENTER" text="Builds">
					<Anchor point="TOP" relativeTo="$(parent)" relativePoint="TOP"/>
					<Dimensions x="130" y="50" />
				</Label>
				<Control name="$(parent)SearchBuild_control"  >
					<Dimensions x="200" y="30" />
					<Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$(parent)Label" offsetY="-3" offsetX="100"/>
					<Controls>
						<Backdrop name="$(parent)TextBG" centerColor="000000" edgeColor="AAAAAA">
							<AnchorFill />
							<Edge edgeSize="2" />
						</Backdrop>
						<EditBox name="SearchBuild_edit" inherits="ZO_DefaultEditForBackdrop ZO_EditDefaultText" maxInputCharacters="100"  editEnabled="true" multiLine="false" newLineEnabled="false"   >
							<Dimensions x="196" y="22" />
							<Anchor point="CENTER" relativePoint="CENTER" relativeTo="$(parent)"  />
						</EditBox>
					</Controls>
				</Control>
				<Button name="$(parent)SearchBuild_btn" alpha="1">
					<Anchor point="TOPLEFT" relativePoint="TOPRIGHT"  relativeTo="$(parent)SearchBuild_control" offsetY="-5" />
					<Textures normal="/esoui/art/miscellaneous/search_icon.dds"  mouseOver="/esoui/art/miscellaneous/search_icon.dds" ></Textures>
					<Dimensions x="40" y="40" />
					<OnMouseDown>SearchBuilds(SearchBuild_edit:GetText())</OnMouseDown>
				</Button>
				<Control name="$(parent)Tabella">
					<Anchor point="TOP" relativePoint="BOTTOM" relativeTo="$(parent)Label" />
					<Dimensions x="900" y="200" />
					<Controls>
						<!--Header-->
						<Control name="$(parent)print_Row0" >
							<Dimensions y="40" x="900" />
							<Anchor point="TOPLEFT" relativePoint="TOPLEFT" relativeTo="$(parent)" />
							<Controls>
								<Control name="$(parent)NoBuilds"  >
									<Anchor point="TOPLEFT" relativePoint="TOPLEFT" relativeTo="$(parent)" offsetX="30" offsetY="40" />
									<Dimensions x="850" y="50" />
									<Controls>
										<Label name="$(parent)Label" font="ZoFontWinH1" inheritAlpha="true" color="FFF11C" wrapMode="TRUNCATE" verticalAlignment="CENTER" horizontalAlignment="CENTER" text="">
											<Anchor point="TOP" relativePoint="TOP" offsetX="0" offsetY="0" relativeTo="$(parent)" />
											<Dimensions x="850" y="50" />
										</Label>
									<Backdrop name="$(parent)BG" inherits="ZO_DefaultBackdrop" />
									</Controls>
								</Control>
								<Control name="$(parent)Colonna0"  >
									<Anchor point="TOPLEFT" relativePoint="TOPLEFT" relativeTo="$(parent)" />
									<Dimensions x="120" y="20" />
									<Controls>
										<Button name="$(parent)DateSortBtn" alpha="1">
											<Anchor point="LEFT" relativeTo="$(parent)" relativePoint="LEFT" />
											<Dimensions x="120" y="20" />
											<OnMouseDown>SortBuildsByDate()</OnMouseDown>
											<Controls>
												<Label name="$(parent)Label" font="ZoFontWinH3" inheritAlpha="true" color="FFFFFF" wrapMode="TRUNCATE" verticalAlignment="CENTER" horizontalAlignment="LEFT" text="Data">
													<Dimensions x="100" y="20" />
													<Anchor point="LEFT" relativeTo="$(parent)" relativePoint="LEFT" />
												</Label>
											</Controls>
										</Button>	
									</Controls>
								</Control>
								<Control name="$(parent)Colonna1"  >
									<Anchor point="TOPLEFT" relativePoint="TOPRIGHT" offsetX="0" relativeTo="$(parent)Colonna0" />
									<Dimensions x="80" y="20" />
									<Controls>
										<Button name="$(parent)TargetSortBtn" alpha="1">
											<Anchor point="LEFT" relativeTo="$(parent)" relativePoint="LEFT" />
											<Dimensions x="80" y="20" />
											<OnMouseDown>SortBuildsByTarget()</OnMouseDown>
											<Controls>
												<Label name="$(parent)Label" font="ZoFontWinH3" inheritAlpha="true" color="FFFFFF" wrapMode="TRUNCATE" verticalAlignment="CENTER" horizontalAlignment="LEFT" text="Target">
													<Dimensions x="80" y="20" />
													<Anchor point="LEFT" relativeTo="$(parent)" relativePoint="LEFT" />
												</Label>
											</Controls>
										</Button>	
									</Controls>
								</Control>
								<Control name="$(parent)Colonna2"  >
									<Anchor point="TOPLEFT" relativePoint="TOPRIGHT" offsetX="0" relativeTo="$(parent)Colonna1" />
									<Dimensions x="100" y="20" />
									<Controls>
										<Button name="$(parent)OwnerSortBtn" alpha="1">
											<Anchor point="LEFT" relativeTo="$(parent)" relativePoint="LEFT" />
											<Dimensions x="100" y="20" />
											<OnMouseDown>SortBuildsByOwner()</OnMouseDown>
											<Controls>
												<Label name="$(parent)Label" font="ZoFontWinH3" inheritAlpha="true" color="FFFFFF" wrapMode="TRUNCATE" verticalAlignment="CENTER" horizontalAlignment="LEFT" text="Autore">
													<Dimensions x="200" y="20" />
													<Anchor point="LEFT" relativeTo="$(parent)" relativePoint="LEFT" />
												</Label>
											</Controls>
										</Button>
									</Controls>
								</Control>
								<Control name="$(parent)Colonna3"  >
									<Anchor point="TOPLEFT" relativePoint="TOPRIGHT" offsetX="0" relativeTo="$(parent)Colonna2" />
									<Dimensions x="200" y="20" />
									<Controls>
										<Button name="$(parent)NameSortBtn" alpha="1">
											<Anchor point="LEFT" relativeTo="$(parent)" relativePoint="LEFT" />
											<Dimensions x="200" y="20" />
											<OnMouseDown>SortBuildsByName()</OnMouseDown>
											<Controls>
												<Label name="$(parent)Label" font="ZoFontWinH3" inheritAlpha="true" color="FFFFFF" wrapMode="TRUNCATE" verticalAlignment="CENTER" horizontalAlignment="LEFT" text="Nome">
													<Dimensions x="200" y="20" />
													<Anchor point="LEFT" relativeTo="$(parent)" relativePoint="LEFT" />
												</Label>
											</Controls>
										</Button>	
									</Controls>
								</Control>
								<Control name="$(parent)Colonna4"  >
									<Anchor point="TOPLEFT" relativePoint="TOPRIGHT" offsetX="0" relativeTo="$(parent)Colonna3" />
									<Dimensions x="80" y="20" />
									<Controls>
										<Button name="$(parent)RaceSortBtn" alpha="1">
											<Anchor point="LEFT" relativeTo="$(parent)" relativePoint="LEFT" />
											<Dimensions x="80" y="20" />
											<OnMouseDown>SortBuildsByRace()</OnMouseDown>
											<Controls>
												<Label name="$(parent)Label" font="ZoFontWinH3" inheritAlpha="true" color="FFFFFF" wrapMode="TRUNCATE" verticalAlignment="CENTER" horizontalAlignment="LEFT" text="Razza">
													<Dimensions x="80" y="20" />
													<Anchor point="LEFT" relativeTo="$(parent)" relativePoint="LEFT" />
												</Label>
											</Controls>
										</Button>	
									</Controls>
								</Control>
								<Control name="$(parent)Colonna5"  >
									<Anchor point="TOPLEFT" relativePoint="TOPRIGHT" offsetX="0" relativeTo="$(parent)Colonna4" />
									<Dimensions x="80" y="20" />
									<Controls>
										<Button name="$(parent)ClassSortBtn" alpha="1">
											<Anchor point="LEFT" relativeTo="$(parent)" relativePoint="LEFT" />
											<Dimensions x="80" y="20" />
											<OnMouseDown>SortBuildsByClass()</OnMouseDown>
											<Controls>
												<Label name="$(parent)Label" font="ZoFontWinH3" inheritAlpha="true" color="FFFFFF" wrapMode="TRUNCATE" verticalAlignment="CENTER" horizontalAlignment="LEFT" text="Classe">
													<Dimensions x="80" y="20" />
													<Anchor point="LEFT" relativeTo="$(parent)" relativePoint="LEFT" />
												</Label>
											</Controls>
										</Button>	
									</Controls>
								</Control>
								<Control name="$(parent)Colonna6"  >
									<Anchor point="TOPLEFT" relativePoint="TOPRIGHT" offsetX="0" relativeTo="$(parent)Colonna5" />
									<Dimensions x="150" y="20" />
									<Controls>
										<Button name="$(parent)RatingSortBtn" alpha="1">
											<Anchor point="LEFT" relativeTo="$(parent)" relativePoint="LEFT" />
											<Dimensions x="150" y="20" />
											<OnMouseDown>SortBuildsByRating()</OnMouseDown>
											<Controls>
												<Label name="$(parent)Label" font="ZoFontWinH3" inheritAlpha="true" color="FFFFFF" wrapMode="TRUNCATE" verticalAlignment="CENTER" horizontalAlignment="LEFT" text="Valutazione">
													<Dimensions x="150" y="20" />
													<Anchor point="LEFT" relativeTo="$(parent)" relativePoint="LEFT" />
												</Label>
											</Controls>
										</Button>
									</Controls>
								</Control>
							</Controls>
						</Control>
						<!-- Rows added here dynamically -->
					</Controls>
				</Control>
			</Controls>
		</Control>
		<!-- /BUILDS LIST -->

		<!-- RATING POINT TEMPLATE  -->
		<Control name="BuildsRatingTemplateFull" virtual="true">
			<Dimensions y="24" x="24" />
			<Anchor point="TOPLEFT" relativePoint="TOPLEFT" relativeTo="$(parent)" offsetY="5" />
			<Controls>
				<Texture name="$(parent)Texture" inheritAlpha="true">
					<Dimensions x="24" y="24" />
					<Anchor point="LEFT" relativeTo="$(parent)" relativePoint="LEFT" />
				</Texture>
			</Controls>
		</Control>
		<!-- /RATING POINT TEMPLATE -->

		<!-- BUILDS ROW TEMPLATE -->
		<Control name="BuildsDynamicRow" virtual="true">
			<Dimensions y="40" x="900" />
			<Anchor point="TOPLEFT" relativePoint="TOPLEFT" relativeTo="$(parent)" />
			<Controls>
				<Control name="$(parent)Colonna0">
					<Anchor point="TOPLEFT" relativePoint="TOPLEFT" relativeTo="$(parent)" />
					<Dimensions x="120" y="40" />
					<Controls>
						<Label name="$(parent)Label" font="ZoFontWinH5" inheritAlpha="true" color="FFFFFF" wrapMode="TRUNCATE" verticalAlignment="CENTER" horizontalAlignment="LEFT" text="Data">
							<Dimensions x="120" y="40" />
							<Anchor point="LEFT" relativeTo="$(parent)" relativePoint="LEFT" />
						</Label>
					</Controls>
				</Control>
				<Control name="$(parent)Colonna1">
					<Anchor point="TOPLEFT" relativePoint="TOPRIGHT" offsetX="0" relativeTo="$(parent)Colonna0" />
					<Dimensions x="80" y="40" />
					<Controls>
						<Label name="$(parent)Label" font="ZoFontWinH5" inheritAlpha="true" color="FFFFFF" wrapMode="TRUNCATE" verticalAlignment="CENTER" horizontalAlignment="LEFT" text="Target">
							<Dimensions x="80" y="40" />
							<Anchor point="LEFT" relativeTo="$(parent)" relativePoint="LEFT" />
						</Label>
					</Controls>
				</Control>
				<Control name="$(parent)Colonna2"  >
					<Anchor point="TOPLEFT" relativePoint="TOPRIGHT" offsetX="0" relativeTo="$(parent)Colonna1" />
					<Dimensions x="100" y="40" />
					<Controls>
						<Label name="$(parent)Label" font="ZoFontWinH5" inheritAlpha="true" color="FFFFFF" wrapMode="TRUNCATE" verticalAlignment="CENTER" horizontalAlignment="LEFT" text="Autore">
							<Dimensions x="100" y="40" />
							<Anchor point="LEFT" relativeTo="$(parent)" relativePoint="LEFT" />
						</Label>
					</Controls>
				</Control>
				<Control name="$(parent)Colonna3">
					<Anchor point="TOPLEFT" relativePoint="TOPRIGHT" offsetX="0" relativeTo="$(parent)Colonna2" />
					<Dimensions x="200" y="40" />
					<Controls>
						<Button name="$(parent)NameButton">
							<Anchor point="LEFT" relativeTo="$(parent)" relativePoint="LEFT" />
							<MouseButton button="2" enabled="true"/>
							<MouseButton button="1" enabled="true"/>
							<OnMouseDown>OpenBuildDetails(self, "TuiBuilds")</OnMouseDown>
							<Dimensions x="200" y="40" />
							<Controls>
								<Label name="$(parent)Label" font="ZoFontWinH5" inheritAlpha="true" color="FFFFFF" wrapMode="TRUNCATE" verticalAlignment="CENTER" horizontalAlignment="LEFT" text="Nome">
									<Dimensions x="200" y="24" />
									<Anchor point="LEFT" relativeTo="$(parent)" relativePoint="LEFT" />
								</Label>
								<Label name="$(parent)Label_BuildID" hidden="true" text="Id">
									<Dimensions x="0" y="0" />
									<Anchor point="LEFT" relativeTo="$(parent)" relativePoint="LEFT" />
								</Label>
							</Controls>
						</Button>
					</Controls>
				</Control>
				<Control name="$(parent)Colonna4">
					<Anchor point="TOPLEFT" relativePoint="TOPRIGHT" offsetX="0" relativeTo="$(parent)Colonna3" />
					<Dimensions x="80" y="24" />
					<Controls>
						<Texture name="$(parent)RaceTexture">
							<Dimensions x="24" y="24" />
							<Anchor point="TOPLEFT" relativeTo="$(parent)" relativePoint="TOPLEFT" />
						</Texture>
						<!--Tooltip name="$(parent)RaceTooltip" font="ZoFontWinH5">
							<Dimensions x="64" y="64" />
							<Anchor point="LEFT" relativeTo="$(parent)RaceTexture" relativePoint="LEFT" />
						</Tooltip-->
						<!--Label name="$(parent)Label" font="ZoFontWinH5" inheritAlpha="true" color="FFFFFF" wrapMode="TRUNCATE" verticalAlignment="CENTER" horizontalAlignment="LEFT" text="Razza">
							<Dimensions x="150" y="20" />
							<Anchor point="LEFT" relativeTo="$(parent)" relativePoint="LEFT" />
						</Label-->
					</Controls>
				</Control>
				<Control name="$(parent)Colonna5">
					<Anchor point="TOPLEFT" relativePoint="TOPRIGHT" offsetX="0" offsetY="0" relativeTo="$(parent)Colonna4" />
					<Dimensions x="80" y="40" />
					<Controls>
						<Texture name="$(parent)ClassTexture">
							<Dimensions x="24" y="24" />
							<Anchor point="TOPLEFT" relativeTo="$(parent)" relativePoint="TOPLEFT" />
						</Texture>
						<!--Tooltip name="$(parent)ClassTooltip" font="ZoFontWinH5">
							<Dimensions x="64" y="64" />
							<Anchor point="LEFT" relativeTo="$(parent)ClassTexture" relativePoint="LEFT" />
						</Tooltip-->
						<!--Label name="$(parent)Label" font="ZoFontWinH5" inheritAlpha="true" color="FFFFFF" wrapMode="TRUNCATE" verticalAlignment="CENTER" horizontalAlignment="LEFT" text="Classe">
							<Dimensions x="150" y="20" />
							<Anchor point="LEFT" relativeTo="$(parent)" relativePoint="LEFT" />
						</Label-->
					</Controls>
				</Control>
				<Control name="$(parent)Colonna6">
					<Anchor point="TOPLEFT" relativePoint="TOPRIGHT" offsetX="0" relativeTo="$(parent)Colonna5" />
					<Dimensions x="120" y="24" />
					<Controls>
						<Texture name="$(parent)Rating1">
							<Dimensions x="24" y="24" />
							<Anchor point="TOPLEFT" relativeTo="$(parent)" relativePoint="TOPLEFT" offsetX="0" offsetY="0" />
						</Texture>
						<Texture name="$(parent)Rating2">
							<Dimensions x="24" y="24" />
							<Anchor point="TOPLEFT" relativeTo="$(parent)Rating1" relativePoint="TOPRIGHT" />
						</Texture>
						<Texture name="$(parent)Rating3">
							<Dimensions x="24" y="24" />
							<Anchor point="TOPLEFT" relativeTo="$(parent)Rating2" relativePoint="TOPRIGHT" />
						</Texture>
						<Texture name="$(parent)Rating4">
							<Dimensions x="24" y="24" />
							<Anchor point="TOPLEFT" relativeTo="$(parent)Rating3" relativePoint="TOPRIGHT" />
						</Texture>
						<Texture name="$(parent)Rating5">
							<Dimensions x="24" y="24" />
							<Anchor point="TOPLEFT" relativeTo="$(parent)Rating4" relativePoint="TOPRIGHT" />
						</Texture>
						<!--Label name="$(parent)Label" font="ZoFontWinH5" inheritAlpha="true" color="FFFFFF" wrapMode="TRUNCATE" verticalAlignment="CENTER" horizontalAlignment="RIGHT" text="Valutazione">
							<Dimensions x="0" y="0" />
							<Anchor point="LEFT" relativeTo="$(parent)" relativePoint="LEFT" />
						</Label-->
					</Controls>
				</Control>
			</Controls>
		</Control>
		<!-- /BUILDS ROW TEMPLATE -->

		<!-- BUILD DETAILS -->
		<Control name="DynamicScrollPageBuildDetails" virtual="true">
			<Anchor point="TOP" relativePoint="TOP" offsetX="0" relativeTo="$(parent)" />
			<Dimensions x="900" y="0" />
			<Controls>
				<Label name="$(parent)Label"  font="ZoFontWinH1" inheritAlpha="true" color="FFFFFF" wrapMode="TRUNCATE" verticalAlignment="TOP" horizontalAlignment="CENTER" text="Dettagli Build">
					<Anchor point="TOPCENTER" relativeTo="$(parent)" relativePoint="TOPCENTER" />
					<Dimensions x="900" y="50" />
				</Label>
				<Control name="$(parent)Content">
					<Anchor point="TOP" relativePoint="BOTTOM" relativeTo="$(parent)Label" />
					<Dimensions x="900" y="1105" />
					<Controls>
						<Label name="$(parent)Name" height="10" font="ZoFontWinH2" inheritAlpha="false" alpha="1" color="FFFFFF" wrapMode="TRUNCATE" verticalAlignment="CENTER" horizontalAlignment="LEFT">
							<Anchor point="TOPLEFT" relativeTo="$(parent)" relativePoint="TOPLEFT" offsetX="30"/>
							<Dimensions x="850" y="35" />
						</Label>
						<Label name="$(parent)Author" height="10" font="ZoFontWinH3" inheritAlpha="false" alpha="1" color="E6E6E6" wrapMode="TRUNCATE" verticalAlignment="CENTER" horizontalAlignment="LEFT">
							<Anchor point="TOPLEFT" relativeTo="$(parent)Name" relativePoint="TOPLEFT" offsetY="35"/>
							<Dimensions x="600" y="35" />
						</Label>
						<Control name="$(parent)Rating">
							<Anchor point="TOPLEFT" relativeTo="$(parent)Name" relativePoint="TOPLEFT" offsetY="50" offsetX="730" />
							<Dimensions x="120" y="24" />
							<Controls>
								<Texture name="$(parent)Rating1">
									<Dimensions x="24" y="24" />
									<Anchor point="TOPLEFT" relativeTo="$(parent)" relativePoint="TOPLEFT" offsetX="0" offsetY="0" />
								</Texture>
								<Texture name="$(parent)Rating2">
									<Dimensions x="24" y="24" />
									<Anchor point="TOPLEFT" relativeTo="$(parent)Rating1" relativePoint="TOPRIGHT" />
								</Texture>
								<Texture name="$(parent)Rating3">
									<Dimensions x="24" y="24" />
									<Anchor point="TOPLEFT" relativeTo="$(parent)Rating2" relativePoint="TOPRIGHT" />
								</Texture>
								<Texture name="$(parent)Rating4">
									<Dimensions x="24" y="24" />
									<Anchor point="TOPLEFT" relativeTo="$(parent)Rating3" relativePoint="TOPRIGHT" />
								</Texture>
								<Texture name="$(parent)Rating5">
									<Dimensions x="24" y="24" />
									<Anchor point="TOPLEFT" relativeTo="$(parent)Rating4" relativePoint="TOPRIGHT" />
								</Texture>
								<!--Label name="$(parent)Label" font="ZoFontWinH5" inheritAlpha="true" color="FFFFFF" wrapMode="TRUNCATE" verticalAlignment="CENTER" horizontalAlignment="RIGHT" text="Valutazione">
									<Dimensions x="0" y="0" />
									<Anchor point="LEFT" relativeTo="$(parent)" relativePoint="LEFT" />
								</Label-->
							</Controls>
						</Control>
						<Texture name="$(parent)RaceTexture">
							<Dimensions x="48" y="48" />
							<Anchor point="TOPLEFT" relativeTo="$(parent)Author" relativePoint="TOPLEFT" offsetY="50" />
						</Texture>
						<Label name="$(parent)RaceLabel" height="10" font="ZoFontWinH3" inheritAlpha="false" alpha="1" color="FFFFFF" wrapMode="TRUNCATE" verticalAlignment="CENTER" horizontalAlignment="LEFT">
							<Anchor point="TOPLEFT" relativeTo="$(parent)RaceTexture" relativePoint="TOPLEFT" offsetX="70"/>
							<Dimensions x="250" y="48" />
						</Label>
						<Texture name="$(parent)ClassTexture">
							<Dimensions x="48" y="48" />
							<Anchor point="TOPLEFT" relativeTo="$(parent)RaceTexture" relativePoint="TOPLEFT" offsetY="60" />
						</Texture>
						<Label name="$(parent)ClassLabel" height="10" font="ZoFontWinH3" inheritAlpha="false" alpha="1" color="FFFFFF" wrapMode="TRUNCATE" verticalAlignment="TOP" horizontalAlignment="LEFT">
							<Anchor point="TOPLEFT" relativeTo="$(parent)ClassTexture" relativePoint="TOPLEFT" offsetX="70"/>
							<Dimensions x="250" y="48" />
						</Label>
						<Label name="$(parent)Items" hidden="true" height="10" font="ZoFontWinH3" inheritAlpha="false" alpha="1" color="FFFFFF" wrapMode="TRUNCATE" verticalAlignment="CENTER" horizontalAlignment="LEFT">
							<Anchor point="TOPLEFT" relativeTo="$(parent)ClassTexture" relativePoint="TOPLEFT" offsetX="0" offsetY="80"/>
							<Dimensions x="850" y="0" />
						</Label>
						<Control name="$(parent)Slots">
							<Anchor point="TOPLEFT" relativePoint="TOPLEFT" relativeTo="$(parent)ClassTexture" offsetY="80" />
							<Dimensions x="850" y="0" />
							<Controls>
								<!-- EQUIP_SLOT_HEAD -->
								<Control name="$(parent)Slot0">
									<Anchor point="TOPLEFT" relativeTo="$(parent)" relativePoint="TOPLEFT" />
									<Dimensions x="200" y="48" />
									<Controls>
										<Texture name="$(parent)Texture">
											<Dimensions x="48" y="48" />
											<Anchor point="TOPLEFT" relativeTo="$(parent)" relativePoint="TOPLEFT" />
										</Texture>
										<Label name="$(parent)Label" height="10" font="ZoFontWinH3" inheritAlpha="false" alpha="1" color="FFFFFF" wrapMode="TRUNCATE" verticalAlignment="CENTER" horizontalAlignment="LEFT">
											<Anchor point="TOPLEFT" relativeTo="$(parent)Texture" relativePoint="TOPLEFT" offsetX="70"/>
											<Dimensions x="800" y="48" />
										</Label>
									</Controls>
								</Control>
								<!-- EQUIP_SLOT_CHEST -->
								<Control name="$(parent)Slot2">
									<Anchor point="TOPLEFT" relativeTo="$(parent)Slot0" relativePoint="BOTTOMLEFT" offsetY="10"/>
									<Dimensions x="200" y="48" />
									<Controls>
										<Texture name="$(parent)Texture">
											<Dimensions x="48" y="48" />
											<Anchor point="TOPLEFT" relativeTo="$(parent)" relativePoint="TOPLEFT" />
										</Texture>
										<Label name="$(parent)Label" height="10" font="ZoFontWinH3" inheritAlpha="false" alpha="1" color="FFFFFF" wrapMode="TRUNCATE" verticalAlignment="CENTER" horizontalAlignment="LEFT">
											<Anchor point="TOPLEFT" relativeTo="$(parent)Texture" relativePoint="TOPLEFT" offsetX="70"/>
											<Dimensions x="800" y="48" />
										</Label>
									</Controls>
								</Control>
								<!-- EQUIP_SLOT_SHOULDERS -->
								<Control name="$(parent)Slot3">
									<Anchor point="TOPLEFT" relativeTo="$(parent)Slot2" relativePoint="BOTTOMLEFT" offsetY="10"/>
									<Dimensions x="200" y="48" />
									<Controls>
										<Texture name="$(parent)Texture">
											<Dimensions x="48" y="48" />
											<Anchor point="TOPLEFT" relativeTo="$(parent)" relativePoint="TOPLEFT" />
										</Texture>
										<Label name="$(parent)Label" height="10" font="ZoFontWinH3" inheritAlpha="false" alpha="1" color="FFFFFF" wrapMode="TRUNCATE" verticalAlignment="CENTER" horizontalAlignment="LEFT">
											<Anchor point="TOPLEFT" relativeTo="$(parent)Texture" relativePoint="TOPLEFT" offsetX="70"/>
											<Dimensions x="800" y="48" />
										</Label>
									</Controls>
								</Control>
								<!-- EQUIP_SLOT_HAND -->
								<Control name="$(parent)Slot16">
									<Anchor point="TOPLEFT" relativeTo="$(parent)Slot3" relativePoint="BOTTOMLEFT" offsetY="10"/>
									<Dimensions x="200" y="48" />
									<Controls>
										<Texture name="$(parent)Texture">
											<Dimensions x="48" y="48" />
											<Anchor point="TOPLEFT" relativeTo="$(parent)" relativePoint="TOPLEFT" />
										</Texture>
										<Label name="$(parent)Label" height="10" font="ZoFontWinH3" inheritAlpha="false" alpha="1" color="FFFFFF" wrapMode="TRUNCATE" verticalAlignment="CENTER" horizontalAlignment="LEFT">
											<Anchor point="TOPLEFT" relativeTo="$(parent)Texture" relativePoint="TOPLEFT" offsetX="70"/>
											<Dimensions x="800" y="48" />
										</Label>
									</Controls>
								</Control>
								<!-- EQUIP_SLOT_WAIST -->
								<Control name="$(parent)Slot6">
									<Anchor point="TOPLEFT" relativeTo="$(parent)Slot16" relativePoint="BOTTOMLEFT" offsetY="10"/>
